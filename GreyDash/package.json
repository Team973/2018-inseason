{
    "name": "GreyDash",
    "version": "2.1.0",
    "description": "Robot control dashboard boilerplate for FRC.",
    "main": "src/main.js",
    "scripts": {
        "start": "electron .",
        "lint": "eslint .",
        "postinstall": "electron-builder install-app-deps",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Team973/GreyDash"
    },
    "author": "Team 973 The Greybots <973greybots@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Team973/GreyDash/issues"
    },
    "homepage": "https://github.com/Team973",
    "build": {
        "appId": "com.greybots.${name}",
        "productName": "${name}",
        "copyright": "Copyright Â© 2018 ${author}",
        "mac": {
            "category": "public.app-category.utilities"
        },
        "linux": {
            "category": "Utility",
            "target": [
                "AppImage",
                "deb"
            ]
        },
        "win": {
            "target": "NSIS"
        }
    },
    "devDependencies": {
        "electron-builder": "^19.56.0",
        "eslint": "^4.17.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.8.0"
    },
    "dependencies": {
        "electron": "^1.8.2",
        "material-design-lite": "^1.3.0",
        "smoothie": "^1.34.0",
        "wpilib-nt-client": "^1.7.0"
    }
}
